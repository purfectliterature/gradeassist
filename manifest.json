{
"manifest_version": 2,
    "name": "GradeAssist for Coursemology",
    "description": "Automatically adjusts students' EXP multiplier. Made with love by Phillmont",
    "version": "1.0",
    "content_scripts": [{
        "js": ["content.js"],
        "css": ["toast.css"],
        "matches": [
            "https://coursemology.org/courses/*/assessments/*/submissions/*/edit*",
            "https://www.coursemology.org/courses/*/assessments/*/submissions/*/edit*",
            "http://coursemology.org/courses/*/assessments/*/submissions/*/edit*",
            "http://www.coursemology.org/courses/*/assessments/*/submissions/*/edit*"
        ]
    }],
    "icons": {
        "64": "gradeassist-64.png",
        "128": "gradeassist-128.png",
        "256": "gradeassist-256.png"
    }
}